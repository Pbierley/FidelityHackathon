{
  "name": "vitepackage",
  "private": true,
  "version": "0.0.0",
  "engines": { "node": ">=20.15.1" },
  "volta": { "node": "20.15.1", "npm": "10.8.2" },
  "scripts": {
    "setup": "npm ci && node scripts/prepare-env.js && npm run build",
    "dev": "concurrently -n SERVER,CLIENT -c auto \"npm:server:dev\" \"npm:client:dev\"",
    "server:start": "node server.js",
    "server:dev": "nodemon server.js",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "nodemon": "^3.1.0",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0"
  }
}
